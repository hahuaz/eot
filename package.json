{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:web\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:web": "cd client-web && pnpm dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev:scrape": "cross-env NODE_ENV=development tsx watch src/scripts/daily-scrape.ts",
    "format": "prettier --write .",
    "prepare": "husky",
    "check:ts": "tsc --noEmit && tsc --noEmit --project client-web/tsconfig.json",
    "check:lint": "oxlint",
    "check:prettier": "prettier --write .",
    "check": "concurrently \"pnpm check:ts\" \"pnpm check:lint\" \"pnpm check:prettier\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.4",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "googleapis": "^150.0.1",
    "papaparse": "^5.5.2",
    "puppeteer": "^24.8.2",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.10.0",
    "@types/papaparse": "^5.3.16",
    "add": "^2.0.6",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.32.0",
    "pnpm": "^10.10.0",
    "prettier": "^3.7.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": "prettier --write",
    "**/*.{js,ts,tsx}": "oxlint",
    "client-web/**/*.{ts,tsx}": "bash -c 'tsc --noEmit --project client-web/tsconfig.json'",
    "src/**/*.{ts,tsx}": "bash -c 'tsc --noEmit'"
  }
}
